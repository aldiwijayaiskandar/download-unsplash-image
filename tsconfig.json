{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["es2017"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./src",
    "paths": {
      "@assets": ["./assets"],
      "@assets/*": ["./assets/*"],
      "@constants": ["./constants"],
      "@constants/*": ["./constants/*"],
      "@common": ["./common"],
      "@common/*": ["./common/*"],
      "@components": ["./components"],
      "@components/*": ["./components/*"],
      "@config": ["./config"],
      "@config/*": ["./config/*"],
      "@models": ["./models"],
      "@models/*": ["./models/*"],
      "@providers": ["./providers"],
      "@providers/*": ["./providers/*"],
      "@store": ["./store"],
      "@store/*": ["./store/*"],
      "@screens": ["./screens"],
      "@screens/*": ["./screens/*"],
      "@services": ["./services"],
      "@services/*": ["./services/*"],
      "@utils": ["./utils"],
      "@utils/*": ["./utils/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
